{
    "parser": "babel-eslint",
    "extends": ["airbnb-base", "plugin:prettier/recommended", "plugin:flowtype/recommended"],
    "plugins": [
        "flowtype"
    ],
    "rules": {
        "linebreak-style": 0
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "jest": true,
        "node": true
    }
}